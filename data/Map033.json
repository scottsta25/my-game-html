{"id":1,"name":"EV001","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":241,"indent":0,"parameters":[{"name":"intro 3 - Copy","volume":90,"pitch":100,"pan":0}]},{"code":105,"indent":0,"parameters":[1,false]},{"code":405,"indent":0,"parameters":["You drag yourself toward the cave’s mouth, boots "]},{"code":405,"indent":0,"parameters":["crunching over frost‑slick stone. Behind you, the ice "]},{"code":405,"indent":0,"parameters":["tunnels groan — ancient, shifting, alive in a way that "]},{"code":405,"indent":0,"parameters":["makes your skin crawl. Every breath you exhale turns "]},{"code":405,"indent":0,"parameters":["to mist, drifting upward like lost spirits escaping "]},{"code":405,"indent":0,"parameters":["the dark."]},{"code":405,"indent":0,"parameters":["The Wardens follow in silence, their footsteps echoing "]},{"code":405,"indent":0,"parameters":["through the crystalline chamber. The walls shimmer with "]},{"code":405,"indent":0,"parameters":["trapped reflections — distorted versions of you, "]},{"code":405,"indent":0,"parameters":["flickering like memories that don’t belong to you at "]},{"code":405,"indent":0,"parameters":["all."]},{"code":405,"indent":0,"parameters":["The Woman’s voice drifts from behind."]},{"code":405,"indent":0,"parameters":["The Woman"]},{"code":405,"indent":0,"parameters":["“Do not look back."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["Ice remembers what it swallows.”"]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["A final gust of frigid air sweeps through the cavern,"]},{"code":405,"indent":0,"parameters":["pushing you forward."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["You step into the light."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["The world explodes into white."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["Snow blinds you for a moment, glittering like"]},{"code":405,"indent":0,"parameters":["shattered glass beneath a pale sky."]},{"code":405,"indent":0,"parameters":["The wind howls"]},{"code":405,"indent":0,"parameters":["across the mountainside, carrying flecks of ice that "]},{"code":405,"indent":0,"parameters":["sting your skin. You shield your eyes, blinking until "]},{"code":405,"indent":0,"parameters":["the world sharpens."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["You’re standing on a narrow ledge carved into the side "]},{"code":405,"indent":0,"parameters":["of a towering cliff. Below, the valley stretches "]},{"code":405,"indent":0,"parameters":["endlessly — a frozen expanse of silver and blue."]},{"code":405,"indent":0,"parameters":[" And "]},{"code":405,"indent":0,"parameters":["nestled at its heart…"]},{"code":405,"indent":0,"parameters":["A town."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["Small."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["Quiet."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["Half‑buried in snow."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["Wooden rooftops sag under the weight of frost. "]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["Lanterns flicker behind frosted windows. Smoke rises "]},{"code":405,"indent":0,"parameters":["from chimneys in thin, wavering trails,"]},{"code":405,"indent":0,"parameters":["as if unsure "]},{"code":405,"indent":0,"parameters":["whether it belongs to this world or another."]},{"code":201